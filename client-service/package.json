{
  "name": "appstack-react",
  "version": "1.3.0",
  "description": "AppStack - React Admin & Dashboard Template",
  "author": "Paul Laros <support@bootlab.io>",
  "private": true,
  "scripts": {
    "sass:compile": "node-sass --source-map true --include-path ./src --include-path ./node_modules src/assets/scss/ -o public/css/",
    "sass:autoprefixer": "postcss public/css/*.css --use autoprefixer -d public/css",
    "build:css": "npm-run-all sass:*",
    "build:js": "react-scripts build",
    "watch:css": "nodemon --watch src/assets/scss -e scss -x \"npm run build:css\"",
    "watch:js": "react-scripts start",
    "start": "npm-run-all -p build:css watch:*",
    "build": "npm-run-all -p build:*"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    "> 0.25%",
    "ie >= 11",
    "not op_mini all",
    "not dead"
  ],
